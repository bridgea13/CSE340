<% if (title) { %>
    <h1><%= title %></h1>
    <% } else { res.redirect('/') } %>
    <%- messages() %> <% if (errors) { %>
    <ul class="notice">
        <% errors.array().forEach(error => { %>
        <li><%= error.msg %></li>
        <% }) %>
    </ul>
    <% } %>

    
<section class="management_view">
    <ul>
        <li><a href="/inventory/addClassification">Add New Classification</a></li>
        <li><a href="/inventory/add-inventory">Add New Vehicle</a></li>
    </ul>
    <h2>Manage Inventory</h2>
    <p>Choose a classification to see those inventory items.</p>
    <%- classList %>

    <table id="inventoryDisplay"></table>

    <noscript>Javascript must be enabled to use this page.</noscript>
</section>
<script src="../../js/inventory.js"></script>